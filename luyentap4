<body>
    <script>
        let a = 0;
        let b = 1;
        let fibon = a
        
        for (i = 0; i < 20; i++) {
            fibon = a + b;
            a = b;
            b = fibon
            if (fibon %5==0){
                break
            }
           
        }document.write(" số fibon chia hết cho 5 là " +fibon)
    </script>
</body>

