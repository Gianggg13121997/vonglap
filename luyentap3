    <input type="text" placeholder="nhập gì đó" id="n">
    <button onclick="fibon()"> check </button>;
    <script>
        function fibon(){
        let n = document.getElementById("n").value;
        let a = 0;
        let b = 1;
        for ( i = 0; i<n; i++){
             fibonnacci = a;
             a = b;
             b =  fibonnacci+ a
            document.write(fibonnacci + ",");
        }
        }
    </script>
